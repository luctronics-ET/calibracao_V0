{
  "name": "calibracao/skeleton",
  "description": "Skeleton Laravel-like project for calibration management",
  "type": "project",
  "require": {
    "php": "^8.0",
    "laravel/framework": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}